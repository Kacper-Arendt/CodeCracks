{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "tsup src/index.ts --format cjs --watch --onSuccess \"node dist/index.js\"",
    "lint": "tsc --noEmit && TIMING=1 eslint \"src/**/*.ts*\"",
    "start": "node dist/index.js",
    "test": "jest --detectOpenHandles",
    "build:start": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --external:express --external:cors && node dist/index.js"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "dependencies": {
    "@types/express-serve-static-core": "^4.17.33",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "esbuild": "^0.17.8",
    "express": "^4.17.1",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tsup": "^6.2.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.17",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.12",
    "eslint": "^7.32.0",
    "eslint-config-custom-server": "workspace:*",
    "jest": "^26.6.3",
    "jest-presets": "workspace:*",
    "supertest": "^6.2.4",
    "typescript": "^4.8.3"
  }
}
